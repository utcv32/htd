WindUI:Popup({
    Title = "çº¯ v1.10",
    Icon = "rbxassetid://129260712070622",
    IconThemed = true,
    Content = "æ¬¢è¿ä½¿ç”¨çº¯è„šæœ¬",
    Buttons = {
        {
            Title = "è¿›å…¥è„šæœ¬ã€‚",
            Icon = "arrow-right",
            Callback = function() Confirmed = true end,
            Variant = "Primary",
        }
    }
})

repeat wait() until Confirmed

